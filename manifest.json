{
  "manifest_version": 2,
  "name": "MaTerialgram",
  "author": "Alvin Hou",
  "description": "New Material Design for Telegram Web with dark and light theme",
  "version": "1.0.0",
  "icons": {
    "16": "icons/icon-dark.png",
    "128": "icons/icon-dark.png"
  },
  "homepage_url": "https://github.com/CryoliteZ/MaTerialgram",
  "browser_action": {
    "default_icon": "icons/icon-dark.png",
    "default_title": "MaTerialgram",
    "default_popup": "popup/popup.html"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://web.telegram.org/*", "http://web.telegram.org/*"],
      "css": ["material-theme.css"],
      "js": ["script.js"]
    }
  ]
}